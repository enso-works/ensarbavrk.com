{
  "name": "blog",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "make-it-pretty": "prettier --write 'src/**/*.{js,jsx,css,scss,json,md}'",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.5.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.4.6",
    "prettier": "2.5.1",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.22"
  },
  "lint-staged": {
    "*.js": "eslint --cache",
    "*.{js, jsx, css,md, scss}": [
      "next lint --fix",
      "npm run make-it-pretty",
      "git add"
    ]
  }
}
